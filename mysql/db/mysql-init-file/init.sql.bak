create user 'root'@'%' identified with mysql_native_password by 1234;
grant all privileges on *.* to 'root'@'%';

CREATE DATABASE test;
use test;

CREATE TABLE owner (
		oid int primary key auto_increment,
		id varchar(100),
		pw varchar(100),
		name varchar(100),
);

CREATE TABLE shop (
		sid int primary key auto_increment,
		type varchar(100),
		sname varchar(100),
		location varchar(100),
		maximum int,
		oid int,
		foreign key (oid) references owner(oid) on update cascade on delete cascade
);

